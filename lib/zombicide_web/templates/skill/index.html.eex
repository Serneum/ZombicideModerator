<h2>Listing Skills</h2>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>

      <th></th>
    </tr>
  </thead>
  <tbody>
<%= for skill <- @skills do %>
    <tr>
      <td><%= skill.name %></td>
      <td><%= skill.description %></td>
    </tr>
<% end %>
  </tbody>
</table>
